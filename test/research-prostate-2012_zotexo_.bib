	pmid = {20813895},
	keywords = {Cause of Death, Databases, Factual, Europe, Health Services Research, Humans, Incidence, Neoplasms, Registries, Regression analysis, Reproducibility of Results, Survival Rate, United States},
	pages = {573--577}
}

