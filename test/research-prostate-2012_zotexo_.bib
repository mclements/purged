	issn = {1527-3792 (Electronic) 0022-5347 (Linking)},
	url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=18930283},
