	title = {Higher {Rate} of {Toxicity} {With} {No} {Increased} {Efficacy} {When} {Hydroxyurea} {Is} {Added} to a {Regimen} of {Stavudine} {Plus} {Didanosine} and {Nevirapine} in {Primary} {HIV} {Infection}},
