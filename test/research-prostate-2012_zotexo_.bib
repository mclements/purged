	volume = {26},
	issn = {0959-8049 (Print) 0959-8049 (Linking)},
	url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=2150000},
