	keywords = {Adult, Cervical Intraepithelial Neoplasia, Female, Guidelines as Topic, Humans, Mass Screening, Papillomavirus Infections, Polymerase Chain Reaction, Sensitivity and Specificity, Uterine Cervical Neoplasms},
	pages = {516--520}
