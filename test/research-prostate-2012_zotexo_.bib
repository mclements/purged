@article{tan_projection_2010,
	title = {Projection of mesothelioma mortality in {Britain} using {Bayesian} methods},
	volume = {103},
	issn = {1532-1827 (Electronic) 0007-0920 (Linking)},
	url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=20628377},
