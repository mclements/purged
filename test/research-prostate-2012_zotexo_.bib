	journal = {Br J Cancer},
	author = {Moller, H. and Fairley, L. and Coupland, V. and Okello, C. and Green, M. and Forman, D. and Moller, B. and Bray, F.},
	year = {2007},
	note = {9},
	keywords = {Cost of Illness England/epidemiology Female Forecasting Humans Incidence Male Neoplasms/classification/*economics/*epidemiology Registries},
	pages = {1484--8},
