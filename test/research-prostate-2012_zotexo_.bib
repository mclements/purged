	title = {Variation over time of the effects of prognostic factors in a population-based study of colon cancer: comparison of statistical models},
	volume = {150},
	url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=10588079},
	journal = {Am J Epidemiol},
