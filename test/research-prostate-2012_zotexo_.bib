	author = {Bill-Axelson, A. and Holmberg, Lars and Ruutu, Mirja and Garmo, Hans and Stark, Jennifer R and Busch, Christer and Nordling, Stig and Häggman, Michael and Andersson, Swen-Olof and Bratell, Stefan and Spångberg, Anders and Palmgren, Juni and Steineck, Gunnar and Adami, Hans-Olov and Johansson, Jan-Erik},
	month = may,
	year = {2011},
	keywords = {Aged, Age Factors, Follow-Up Studies, Humans, Intention to Treat Analysis, Male, Middle Aged, Neoplasm Metastasis, Neoplasm Staging, Proportional Hazards Models, Prostate, Prostatectomy, Prostatic Neoplasms, Risk, Risk Factors, Survival Analysis, Watchful Waiting},
