	keywords = {Aged, Aged, 80 and over, Cohort Studies, Early Detection of Cancer, Empirical Research, Humans, Male, Middle Aged, Population Surveillance, Prostate-Specific Antigen, Prostatic Neoplasms, Sweden, Tumor Markers, Biological},
