	volume = {1},
	journal = {HIV.Med.},
	author = {Madge, S. and Mocroft, A. and Wilson, D. and Youle, M. and Lipman, M.C. and Phillips, A. and Tyrer, M. and Cozzi-Lepri, A. and Swaden, L. and Johnson, M.A.},
