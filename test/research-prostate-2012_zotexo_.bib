	title = {Correcting the completeness bias of observed prevalence},
	volume = {85},
	url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=10665853},
	journal = {Tumori},
	author = {Corazziari, I. and Mariotto, A. and Capocaccia, R.},
	year = {1999},
